{
    "name": "eew-bot",
    "version": "1.0.0",
    "license": "GPL-3.0-or-later",
    "author": "65_7a",
    "private": "true",
    "scripts": {
        "dev": "nodemon --exec node --inspect=0.0.0.0:9229 -r ts-node/register src/index.ts",
        "start": "node dist/index.js",
        "build": "npx tsc",
        "lint": "eslint \"src/**/*.ts\"",
        "format": "prettier \"src/**/*.ts\" --write",
        "prepare": "husky install",
        "clean": "rimraf dist logs record && node ClearCommands.js"
    },
    "dependencies": {
        "@discordjs/rest": "^0.4.1",
        "axios": "^0.26.1",
        "color-convert": "^2.0.1",
        "dedent": "^0.7.0",
        "discord.js": "^13.6.0",
        "dotenv": "^16.0.0",
        "glob-promise": "^4.2.2",
        "luxon": "^2.3.1",
        "m3u8stream": "^0.8.6",
        "mongoose": "^6.2.8",
        "rimraf": "^3.0.2",
        "winston": "^3.6.0",
        "ws": "^8.5.0"
    },
    "devDependencies": {
        "@types/color-convert": "^2.0.0",
        "@types/dedent": "^0.7.0",
        "@types/glob": "^7.2.0",
        "@types/luxon": "^2.3.1",
        "@types/node": "^17.0.23",
        "@types/ws": "^8.5.3",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "eslint": "^8.11.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.7",
        "nodemon": "^2.0.15",
        "prettier": "^2.6.0",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.2"
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix",
            "prettier --write"
        ]
    }
}
